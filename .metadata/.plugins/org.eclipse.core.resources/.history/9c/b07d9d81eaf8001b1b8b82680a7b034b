package Learning.Selenium;

import java.util.Arrays;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args ) {
	
    	int[] arr={28,3,15,9,17,4,23,2};
    	
    	
    	Arrays.asList(arr).stream().sorted().limit(1).forEach(System.out::println);    	
    	
    	
    }
}
